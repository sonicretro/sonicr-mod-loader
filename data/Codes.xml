    <Code name="Hard Balloon Mode (Need All 17 Balloons!)">
        <CodeLine>
            <!-- If Mode is Balloon Hunt -->
            <Type>ifeq8</Type>
            <Address>007BCB80</Address>
            <Value>3</Value>
            <ValueType>decimal</ValueType>
            <TrueLines>
                <CodeLine>
                    <!-- If Player Number is 1 -->
                    <Type>ifeq8</Type>
                    <Address>007356EC</Address>
                    <Value>1</Value>
                    <ValueType>decimal</ValueType>
                    <TrueLines>
                        <CodeLine>
                            <!-- Change Number of Balloons needed to 17 -->
                            <Type>write32</Type>
                            <Address>0043950A</Address>
                            <Value>17</Value>
                            <ValueType>decimal</ValueType>
                        </CodeLine>
                        <CodeLine>
                            <!-- Change number of exta spawned balloons to 13 (as 17 - 4 = 13) -->
                            <Type>write32</Type>
                            <Address>00422853</Address>
                            <Value>13</Value>
                            <ValueType>decimal</ValueType>
                        </CodeLine>
                        <CodeLine>
                            <!-- Make counter disappear by changing the rules for the HUD element -->
                            <Type>write8</Type>
                            <Address>0043E85B</Address>
                            <Value>5</Value>
                            <ValueType>decimal</ValueType>
                        </CodeLine>
                    </TrueLines>
                    <FalseLines>
                        <!-- If Player Number is not 1, since we are in Balloon Mode to even run this -->
                        <CodeLine>
                            <!-- Change Number of Balloons needed back to 5 -->
                            <Type>write32</Type>
                            <Address>0043950A</Address>
                            <Value>5</Value>
                            <ValueType>decimal</ValueType>
                        </CodeLine>
                        <CodeLine>
                            <!-- Change number of extra balloons spawned back to 1 -->
                            <Type>write32</Type>
                            <Address>00422853</Address>
                            <Value>1</Value>
                            <ValueType>decimal</ValueType>
                        </CodeLine>
                        <CodeLine>
                            <!-- Make counters appear by unchanging the rules for the HUD element -->
                            <Type>write8</Type>
                            <Address>0043E85B</Address>
                            <Value>3</Value>
                            <ValueType>decimal</ValueType>
                        </CodeLine>
                    </FalseLines>
                </CodeLine>
            </TrueLines>
        </CodeLine>
    </Code>
